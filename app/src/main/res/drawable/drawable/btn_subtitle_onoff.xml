<?xml version="1.0" encoding="UTF-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:state_enabled="false"
		android:drawable="@drawable/subtitle_on_dimmed"/>
	<item
		android:state_checked="true"
		android:drawable="@drawable/subtitle_on_default"/>
	<item
		android:state_checked="false"
		android:drawable="@drawable/subtitle_off_default"/>
	<item
		android:state_pressed="true"
		android:state_checked="false"
		android:drawable="@drawable/subtitle_off_press" />
	<item
		android:state_pressed="false"
		android:state_checked="false"
		android:drawable="@drawable/subtitle_on_press" />
</selector>